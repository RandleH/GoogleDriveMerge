    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Debugger: Example",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--dst", "dummy", "--logging", "debug", "${workspaceFolder}/example/dummy1", "${workspaceFolder}/example/dummy2", "${workspaceFolder}/example/dummy3"]
        }
    ]
}